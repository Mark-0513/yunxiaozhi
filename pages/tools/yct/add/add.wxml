<cu-custom bgColor="bg-white" isBack="true">
  <view slot="back">返回</view>
	<view slot="content">添加羊城通卡</view>
</cu-custom>
<view class='add_yct'>
  <form bindsubmit="formSubmit" bindreset="formReset">
    <view class='inputView'>
      <label>请输入羊城通卡号</label>
      <input name='number' class='info' />
    </view>
    <view class="padding flex flex-direction">
      <button class="cu-btn bg-theme lg" bindtap='add' formType="submit">添加</button>
    </view>
  </form>
</view>