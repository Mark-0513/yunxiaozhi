page{
  width: 100%;
}
.bg{
  z-index: -999;
  position: fixed;
  left: 0;
}
.bg-diy{
  background: #e7f4f0
}
.bg image{
  width:100%;
  height:100%;
}
.clockin{
  padding-top: 180rpx;
}
.cu-btn.lg{
  padding: 0 120rpx;
  letter-spacing: 2px;
}
.number{
  font-size: 60rpx;
  letter-spacing: 2px;
}
.cu-dialog{
  width: 300px;
}
.cu-dialog .bg-img{
  width:300px;
  height:407px;
  background-image: url("https://yunxiaozhi-1251388077.cos.ap-guangzhou.myqcloud.com/mini/other/success.png");
}

.dialog-close{
  position: absolute;
  width: 68rpx;
  height: 68rpx;
  margin-left: -34rpx;
  background: white;
  bottom: 100px;
  left: 50%;
  border-radius: 50%;
}
.dialog-close .icon-close{
  line-height: 68rpx;
}
.dialog-content{
  padding: 110px 0 50px;
}
.success-text{
  font-size: 60rpx;
}
.rank{
  padding: 60rpx 40rpx;
}
.rank-num{
  width: 60rpx;
  position: absolute;
  left: 30rpx;
  text-align: center;
}
.cu-list.menu-avatar>.cu-item>.cu-avatar{
  left: 100rpx;
}
.cu-list.menu-avatar>.cu-item .content {
  left: 216rpx;
  width: calc(100% - 96rpx - 60rpx - 120rpx - 20rpx - 70rpx);
}

.myself .text-grey{
  color: var(--yellow)
}
.options{
  padding: 30rpx 40rpx 20rpx;
}
.option{
  position: fixed;
  right: 0;
  top: 200rpx;
}
.option .item{
  border-radius: 10rpx 0 0 10rpx;
  padding: 8rpx 20rpx;
  margin: 32rpx 0;
  position: relative;
}
.option .item button{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: transparent;
  z-index: 1;
}
.you{
  background: #f5f6f7!important;
}