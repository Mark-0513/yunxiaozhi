page{
  background: #fff;
}
.head{
  position: fixed;
  height: 40px;
  width: 100%;
  justify-content: center;
  background: #fff;
}
.left,.right{
  padding: 0 20px;
  font-size: 22px;
  height: 40px;
  line-height: 40px;
  position: absolute;
  top:0px;
}
.left{
  left: 15px;
}
.right{
  right: 15px;
}
image{
  padding: 10px;
  height: 20px;
  width: 20px;
}
.now_month{
  position: relative; 
  text-align: center;
  line-height: 40px;
  z-index: -1;
} 
.data{
  position: fixed;
  background: #fff;
  top: 40px;
  width: 100%;
  height: 286px;
}
.title{
  width: 100%;
  height: 30px;
  background: #eee;
  display: flex;
  text-align: center;
  line-height: 30px;
  font-size: 12px;
}
.week{
  width: 17.68%;
}
.weekly{
  width: 11.76%;
}
.zhoushu{
  position: relative;
  float: left;
  width: 17.68%;
  text-align: center;
  line-height: 30px;
}
.zhou{
 height: 50px;
 color: #0099CC;
 line-height: 50px;
 border-bottom: 1px solid #f2f4f5;
}
.riqi{
  position: relative;
  float: left;
  width: 82.32%;
  display: block;
  text-align: center;
  line-height: 30px;
}
.ri{
  height: 50px;
  width: 14.28%; 
  line-height: 50px;
  float: left;
  border-bottom: 1px solid #f2f4f5;
}
.num{
  height: 50px;
}
.num .text-df{
  padding-top: 4px;
  height: 26px;
  line-height: 26px;
}
.num .text-xs{
  height: 14px;
  line-height: 14px;
}
.spotAll{
  height: 5px;
  margin: -8px auto;
}
.spot{
  width: 5px;
  height: 5px;
  border-radius: 20px;
  float: left;
}
.spot:not(:first-child){
  margin-left: 2px;
}
.empyt{
  height: 50px;
  float: left;
  border-bottom: 1px solid #f2f4f5;
}
.shijianAll{
  width: 100%;
  float: left;
}
.shijian{
  height: 50px;
  width: 100%;
  line-height: 50px;
  border-bottom: 1px solid #f2f4f5;
  float: left;
  z-index: -10;
}
.li{
  margin-left: 10px;
  float: left;
  overflow: hidden;
  white-space: nowrap;
  text-overflow:ellipsis;
}
.click-btn{
  animation-name: click;
  animation-duration: 0.5s;
  animation-timing-function: ease-in-out;
}
.icon-back,.icon-right{
  border-radius: 100%;
  padding: 16rpx;
}
.thisDay{
  background: #f1f1f1;
}
@keyframes click{
  0%{
    background: #fff;
  }
  50%{
    background: #f1f1f1;
  }
  100%{
    background: #fff;
  }
}
